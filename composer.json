{
    "name": "fullstack/redbird",
    "description": "A Laravel SaaS package with Filament admin panel, user management, and subscription billing",
    "version": "1.0.0",
    "keywords": ["laravel", "saas", "filament", "subscription", "billing", "admin"],
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Fullstack\\Redbird\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Fullstack LLC",
            "email": "hello@fullstack.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "filament/filament": "^3.3",
        "spatie/laravel-permission": "^6.20",
        "laravel/cashier": "^15.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.2",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fullstack\\Redbird\\RedbirdServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
